{
  "expo": {
    "name": "MasterFix",
    "slug": "MasterFix",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/MasterfixLOGO.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/MasterfixLOGO.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.omolounnuoluwafemi.MasterFix"
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-font",
      "expo-asset"
    ],
    "extra": {
      "eas": {
        "projectId": "fce49ef8-d431-40a8-87aa-e0b2671d1cfb"
      }
    }
  }
}
